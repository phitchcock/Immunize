<% @locations.each do |location| %>
  <div class="list-group-item">
    <h4 class="list-group-item-heading"><%=link_to location.name, location %></h4>
    <p class="list-group-item-text"><%= location.street_number %> <%= location.street_name %></p>
    <p class="list-group-item-text"><%= location.city %>, <%= location.state %> <%= location.zip %></p>
    <p class="list-group-item-text"><%= location.date %> <%= location.time %></p>
  </div>
<% end %>
